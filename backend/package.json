{
  "name": "backend",
  "version": "1.0.0",
  "description": "QommerceHub Backend API",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "migrate": "node scripts/migrate.js",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:all": "node scripts/test-all.js",
    "seed:demo": "node scripts/seed-demo.js",
    "demo:setup": "npm run migrate && node scripts/seed-demo.js"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["./__tests__/setup.js"],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "routes/**/*.js",
      "middleware/**/*.js",
      "services/**/*.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@sentry/node": "^8.40.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "stripe": "^20.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "supertest": "^7.2.2"
  }
}
